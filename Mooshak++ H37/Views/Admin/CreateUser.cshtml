@model IEnumerable<Mooshak___H37.Models.Viewmodels.CourseViewModel>
@{
    ViewBag.Title = "CreateUser";
}

<link type="text/css" rel="stylesheet" href="~/Content/Admin.css">

<div class="clearfix">
    <div class="admin-createCourse-buttons">
        <button class="btn btn-primary save-button">Save</button>
    </div>


    <div class="left-portion">
        <form class="form-horizontal">
            <label for="Name">Name</label>
            <input type="text" name="Name" class="form-control" id="Name" /><br />
            <label for="type">Type</label>
            <select name="type" class="form-control" id="type">
                <option value="Teacher">Teacher</option>
                <option value="Student">Student</option>
                <option value="Admin">Admin</option>
            </select>
            <label for="lastlogin">Last Login</label>
            <label name="lastlogin" id="lastlogin">11.</label>
        </form>
        <br />
        <br />
    </div>
</div>